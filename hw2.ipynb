    "result_vowels = \"\"\n",
    "vowels_count = 0\n",
    "for character in text:\n",
    "    if character in vowels:\n",
    "        result_vowels += character\n",
    "        vowels_count += 1\n",
    "\n",
    "print(result_vowels, vowels_count)"
