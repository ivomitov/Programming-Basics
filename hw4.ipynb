   "source": [
    "#余裕のある人に\n",
    "T = int(input())\n",
    "A = list(map(int, input().split()))\n",
    "\n",
    "book_count = 0\n",
    "for book_time in A:\n",
    "    if T - book_time > 0:\n",
    "        book_count += 1\n",
    "        T -= book_time\n",
    "    else:\n",
    "        break\n",
    "\n",
    "print(book_count)"
   ]
  }
